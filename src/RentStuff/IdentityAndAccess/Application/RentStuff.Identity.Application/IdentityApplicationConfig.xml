<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <!-- Services -->
  <object id="AccountApplicationService" type="RentStuff.Identity.Application.Account.AccountApplicationService, RentStuff.Identity.Application">
    <constructor-arg name="accountRepository" ref="AccountRepository"/>
    <constructor-arg name="customEmailService" ref="CustomEmailService"/>
    <constructor-arg name="userTokenProviderService" ref="UserTokenProviderService"/>
  </object>
</objects>