<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="RentStuff.Property.Domain.Model" namespace="RentStuff.Property.Domain.Model.HouseAggregate">
  <class name="Location" table="location" lazy="false">
    <id name="Id">
      <column name="id"/>
      <generator class="native" />
    </id>
    <property name="Latitude" column="latitude"/>
    <property name="Longitude" column="longitude"/>
    <property name="HouseNo" column="house_no"/>
    <property name="StreetNo" column="street_no"/>
    <property name="Area" column="area"/>
    <one-to-one name="House" constrained="true" foreign-key="none" class="House"/>
  </class>
</hibernate-mapping>