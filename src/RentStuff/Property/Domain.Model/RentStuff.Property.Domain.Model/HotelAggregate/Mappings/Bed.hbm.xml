<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RentStuff.Property.Domain.Model" namespace="RentStuff.Property.Domain.Model.HotelAggregate">
  <class name="Bed" table="bed" lazy="false">
    <id name="Id">
      <column name="id"/>
      <generator class="assigned" />
    </id>
    <property name="BedCount" column="bed_count"/>
    <property name="BedType" column="bed_type"/>
    <many-to-one not-null="true" name="Hotel" column="hotel_id" />
  </class>
</hibernate-mapping>